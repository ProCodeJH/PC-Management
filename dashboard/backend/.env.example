# Enterprise PC Management Dashboard
# v20.0 Environment Configuration Template
# Copy to .env and update values

# Server
PORT=3001
NODE_ENV=development

# Security — CHANGE THESE IN PRODUCTION!
JWT_SECRET=CHANGE_ME_generate_with_npm_run_generate-secrets
CREDENTIAL_ENCRYPTION_KEY=CHANGE_ME_16_hex_chars
DEFAULT_ADMIN_PASSWORD=CHANGE_ME_strong_password

# CORS — restrict to your domain in production
CORS_ORIGIN=http://localhost:3001

# Logging
LOG_LEVEL=debug

# Workers (cluster mode)
MAX_WORKERS=4
