{
    "name": "enterprise-pc-dashboard-backend",
    "version": "20.0.0",
    "description": "Central PC Management Dashboard - Backend API (v20.0 Ultimate Performance)",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "cluster": "node cluster.js",
        "generate-secrets": "node -e \"console.log('JWT_SECRET=' + require('crypto').randomBytes(64).toString('hex')); console.log('CREDENTIAL_ENCRYPTION_KEY=' + require('crypto').randomBytes(16).toString('hex'))\"",
        "health": "curl -s http://localhost:3001/api/health | node -e \"process.stdin.on('data',d=>console.log(JSON.parse(d)))\"",
        "metrics": "curl -s http://localhost:3001/api/metrics",
        "lint": "echo 'No linter configured'",
        "test": "echo 'No tests configured'"
    },
    "keywords": [
        "pc-management",
        "dashboard",
        "enterprise",
        "real-time",
        "websocket"
    ],
    "author": "Coding Ssok Academy",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.95.3",
        "bcrypt": "^5.1.0",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.0",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.0",
        "helmet": "^7.0.0",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "socket.io": "^4.6.1",
        "sqlite3": "^5.1.6",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    }
}
